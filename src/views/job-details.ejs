
		<meta charset="utf-8">
        <title>Detalle de Trabajo</title>
        <link rel="stylesheet" href="/styles/st.css">
        <link rel="stylesheet" href="/styles/job_details.css">
        
        <div class="contenedor details_job-box">  
            <!--Buscador-->
            <div class="buscador">
                <form>
                    <div class="texto form-group mx-sm-3 mb-2">
                        <input class="form-control" type="text" placeholder="Live search" />
                    </div>
                    <button type="submit" class="boton botonsearch btn btn-primary mb-2">Search</button>
                </form>
                <button type="submit" class="boton botonpost btn btn-success">Post a Job</button>
            </div>

            <div class="principal">

                

                <!--logo-->
                <div class="imagen">
                    <img id="logo" class="logo" src=" ">
                </div>
                
                <!--Compañia-->
                <h1 id="compania"></h1>
                <!--Ubicacion-->
                <h5 id="ubicacion"></h5><hr>
                
                <!--Posicion y Tipo-->
                <h5 id="desc_trabajo"></h5><hr>
                
                <!--Descripcion del trabajo-->
                <h4 class="text-center">Descripción: </h4>
                <p id="descripcion"></p>
            </div>
        </div>
        <script type="text/javascript" src="/scripts/job_details.js"></script>

